{
	"name": "Profit Calculators",
	"description": "Calculate the Crypto profit",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "./js/background.js"
	},
	"permissions": ["storage", "activeTab", "tabs", "scripting"],
	"action": {
		"default_title": "Count Profit",
    	"default_popup": "./popup.html",
		"default_icon": {
			"16": "./images/profit-16.png",
			"32": "./images/profit-32.png",
			"48": "./images/profit-48.png"
		}
	},
	"icons": {
		"16": "./images/profit-16.png",
		"32": "./images/profit-32.png",
		"48": "./images/profit-48.png"
	}
}